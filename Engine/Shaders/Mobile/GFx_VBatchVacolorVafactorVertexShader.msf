UNIFORM( vec4, BatchFloat[30] );
ATTRIBUTE( vec4, Position, POSITION );
ATTRIBUTE( vec4, Color, COLOR0 );
ATTRIBUTE( vec4, VertexColor, COLOR1 );
OUT_VARYING_LOW( vec4, color, COLOR0 );
OUT_VARYING_DEFAULT( vec4, factor, COLOR1 );
OUT_BUILTIN( vec4, gl_Position, POSITION )
void main() {
// E position


  gl_Position = Position;
  gl_Position.x = dot(Position, BatchFloat[int(0.1+2.0*(255.01*float(VertexColor.b))+(0.0))]);
  gl_Position.y = dot(Position, BatchFloat[int(0.1+2.0*(255.01*float(VertexColor.b))+(1.0))]);  
// E Vacolor
  color = Color;
// E Vafactor
  factor = VertexColor;

}

