var prefDir = fl.configURI + 'Scaleform/preferences/';function loadPreferences(p_file) {		var file = 'prefs.plist';        var prefURI = prefDir + file;        if (FLfile.exists(prefURI)) {                var prefs = FLfile.read(prefURI);                return prefs;        } else {                if (!FLfile.exists(prefDir)) { FLfile.createFolder(prefDir) };                if (FLfile.write(prefURI, "<plist />")) {                        return loadPreferences(file);                } else {                        return false;                }        }}function savePreferences(p_file, p_prefs) {		var file = 'prefs.plist';        var prefURI = prefDir + file;        if (FLfile.write(prefURI, p_prefs)) {                return true;        } else {                return false;        }}function deleteFile(p_file) {        if (p_file == "" || !p_file) { return "</error>"; }        var prefURI = prefDir + p_file;        if (FLfile.remove(prefURI)) {                return true;        } else {                return false;        }}