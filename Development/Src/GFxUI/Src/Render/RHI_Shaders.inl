struct NamedShader
{
    const char*      name;
	unsigned         baseIndex;
	const unsigned*  flags;
};
unsigned FS_base_Flags[] = {4, 0, 8, 1};
const NamedShader NamedShaders[] = {
    {"TextDFA",           FragShaderDesc::FS_FTextDFA,         FS_base_Flags},
    {"TextDFABatch",      FragShaderDesc::FS_FTextDFABatch,    FS_base_Flags},
    {"TextDFAMul",        FragShaderDesc::FS_FTextDFAMul,      FS_base_Flags},
    {"TextDFABatchMul",   FragShaderDesc::FS_FTextDFABatchMul, FS_base_Flags},
    {0, 0, 0}
};

FragShaderType FragShaderDesc::FindStaticShader(const char* name, unsigned flags)
{
	const NamedShader* pfs;
    for (pfs = NamedShaders; pfs->name; pfs++)
	{
	    if (!strcmp(name, pfs->name))
			break;
	}
	if (!pfs->name)
		return FS_None;
	unsigned shader = pfs->baseIndex;
	for (unsigned i = 0; i <= 3; i++)
	    if (flags & (1 << i))
		    shader += pfs->flags[i];
	return (FragShaderType) shader;
}
